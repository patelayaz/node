<% layout(`layouts/boilerplate`) %>
<div class="row">
  <div class="col-8 offset-2">
    <div class="alert alert-danger" role="alert">
      <h4 class="alert-heading"><%= err.message %> </h4>
      <p class="h4">Status code:</p>
      <p><%= err.statusCode %> </p>
      <p class="h4">Stack trace: </p>
      <p><%= err.stack %> </p>
      <hr>
      <p class="mb-0">Try again, maybe?</p>
    </div>
  </div>
</div>
